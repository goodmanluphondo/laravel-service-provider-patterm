{
    "name": "goodmanluphondo/laravel-service-repository-pattern",
    "description": "Service-Repository pattern for your Laravel applications",
    "type": "library",
    "keywords": [
        "laravel",
        "service-repository",
        "pattern",
        "repository",
        "service",
        "pattern",
        "clean-architecture"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Goodman Luphondo",
            "email": "luphondog@icloud.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/console": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "GoodmanLuphondo\\LaravelServiceRepositoryPattern\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoodmanLuphondo\\LaravelServiceRepositoryPattern\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoodmanLuphondo\\LaravelServiceRepositoryPattern\\Providers\\ServiceRepositoryPatternServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.4",
        "phpunit/phpunit": "^12.2"
    }
}
